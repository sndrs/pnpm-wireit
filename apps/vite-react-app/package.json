{
	"name": "vite-react-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "wireit",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview"
	},
	"wireit": {
		"build": {
			"command": "tsc && vite build",
			"files": [
				"tsconfig.json",
				"src/**/*",
				"../../tsconfig.base.json"
			],
			"output": [
				"dist/**/*"
			]
		}
	},
	"dependencies": {
		"@guardian/lib-a": "workspace:^",
		"@guardian/lib-b": "workspace:^",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.2.55",
		"@types/react-dom": "^18.2.19",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"@vitejs/plugin-react": "^4.2.1",
		"eslint": "^8.56.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"typescript": "^5.3.3",
		"vite": "^5.1.1",
		"wireit": "^0.14.4"
	}
}
